{
  "name": "@tic-tac-toe/core",
  "version": "0.0.0",
  "dependencies": {
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.16",
    "@tic-tac-toe/eslint-config": "workspace:*",
    "@tic-tac-toe/typescript-config": "workspace:*",
    "concurrently": "^9.0.0",
    "typescript": "^5.7.3"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "scripts": {
    "build": "concurrently --kill-others-on-fail -p \"[{name}]\" -n \"TSC,SWC\" -c \"blue.bold,yellow.bold\" \"pnpm build:ts\" \"pnpm build:swc\"",
    "build:swc": "swc src -d dist --strip-leading-paths",
    "build:ts": "tsc",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TSC,SWC\" -c \"blue.bold,yellow.bold\" \"pnpm watch:ts\" \"pnpm watch:swc\"",
    "watch:swc": "pnpm build:swc -w",
    "watch:ts": "pnpm build:ts -w --preserveWatchOutput"
  },
  "type": "module"
}
